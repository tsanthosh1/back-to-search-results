{
  "manifest_version": 3,
  "name": "Back to Search Results",
  "version": "1.0",
  "description": "Navigate back to search results using Alt+Shift+Left Arrow",
  "permissions": [
    "tabs",
    "history",
    "webNavigation",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "back-to-search": {
      "suggested_key": {
        "default": "Alt+Shift+Left"
      },
      "description": "Navigate back to search results"
    }
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Back to Search Results"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
